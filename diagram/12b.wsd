
@startuml findMinIn2DArray
title findMinIn2DArray
start
: user input arr2D  ;
: min = arr2D[0][0];
repeat:for (let i = 0; i < arr2D.length; i++)   
    repeat:for (let j = 0; j < arr2D[0].length; j++)     
    if (arr2D[i][j] > min) then (yes ) 
        :min = arr2D[i][j];
    endif 
repeat while(true)
repeat while(true)
:  return min; 
stop
@enduml